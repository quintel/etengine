%h2
  ==#{@historic_serie.key}
  %small= link_to 'Edit', edit_data_historic_serie_path(:id => @historic_serie.id)


%pre== Key: #{@historic_serie.key}

%table
  %thead
    %tr
      %th.tal year
      %th.tal value
  %tbody
    - @historic_serie.year_values.each do |entry|
      %tr
        %td= entry.year
        %td= entry.value

= render '/data/shared/revisions', :versioned => @historic_serie
