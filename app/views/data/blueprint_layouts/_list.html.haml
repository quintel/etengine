#panel
  %input#search{:'data-search_scope' => 'converters', :type => 'search'}

  .tree
    %ul    
      - BlueprintLayout.all.each do |blueprint_layout|
        %li{:'data-search' => [blueprint_layout.id, blueprint_layout.key].flatten.join(' ')}
          %a{:href => data_blueprint_layout_path(:id => blueprint_layout.id), :target => '_blank'}
            %span.id== ##{blueprint_layout.id}
            %span.key= blueprint_layout.key
