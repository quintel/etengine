- breadcrumb "Area: #{@area.area}"

%table
  %tbody
    - Qernel::Area::ATTRIBUTES_USED.each do |column_name|
      %tr
        %th= column_name
        - v = @area.send(column_name)
        %td{:title => v}
          - if v.is_a?(Numeric)
            = auto_number v
          - else
            = v
