#panel
  %input#search{:'data-search_scope' => 'converters', :type => 'search'}
  .tree
    %ul
      = cache 'gquery_list' do
        - Gquery.includes(:gquery_groups).each do |gquery|
          %li{:'data-search' => [gquery.id, gquery.key, gquery.gquery_groups.map{|g| g.group_key.underscore}].flatten.join(' ')}
            %a{:href => data_gquery_path(:id => gquery)}
              %span.id== ##{gquery.id}
              %span.key= gquery.key
              %span.sector= gquery.gquery_groups.first.andand.group_key