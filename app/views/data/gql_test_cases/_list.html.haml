#panel.panel.panel_tree
  .header
    %input#search{:'data-search_scope' => 'converters'}

  .tree
    = cache("admin/gql_test_cases/_list") do
      %ul
        - GqlTestCase.all.each do |gql_test_case|
          %li{:'data-search' => [gql_test_case.id, gql_test_case.name].flatten.join(' ')}
            %a{:href => data_gql_test_case_path(:id => gql_test_case)}
              %span.id== ##{gql_test_case.id}
              %span.key= gql_test_case.name
